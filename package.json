  "lint-staged": {
    "*.js": [
      "./node_modules/.bin/eslint --fix",
      "./node_modules/.bin/prettier --write",
      "git add"
    ],
    "src/**/*.css": [
      "./node_modules/.bin/prettier --write",
      "git add"
    ]
  },
  "license": "MIT",
  "dependencies": {
    "babel-plugin-lodash": "3.3.2",
